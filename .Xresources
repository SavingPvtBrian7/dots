! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #282a36, #bfbfbf, #4d4d4d

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #282a36, #bfbfbf, #282a36, #282a36, #ffffff
rofi.color-active:  #282a36, #7cb8bb, #282a36, #282a36, #7cb8bb
rofi.color-urgent:  #282a36, #dca3a3, #282a36, #282a36, #dca3a3

! Set the desired separator style
rofi.separator-style: solid
